{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "bootstrap@5.0.1",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css"
      ]
    },
    {
      "library": "clipboard@2.0.8",
      "destination": "wwwroot/clipboard/",
      "files": [
        "dist/clipboard.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "plotly.js@2.1.0",
      "destination": "wwwroot/js/plotly.js/"
    },
    {
      "provider": "cdnjs",
      "library": "jstree@3.3.11",
      "destination": "wwwroot/js/jstree/"
    },
    {
      "provider": "cdnjs",
      "library": "clipboard.js@2.0.8",
      "destination": "wwwroot/js/clipboard.js/"
    }
  ]
}