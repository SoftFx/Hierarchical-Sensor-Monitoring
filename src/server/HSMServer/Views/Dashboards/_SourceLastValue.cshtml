@model HSMServer.Model.Dashboards.PanelViewModel

<table class="table d-flex" style="font-size: clamp(1rem, 3vw, 1rem)">
    <tr>
        <th>Color</th>
        <th>Label</th>
        <th>Time</th>
        <th>Value</th>
    </tr>
    @foreach (var i in @Model.Sources.Values)
    {
        @await Html.PartialAsync("_LastValue", i)
    }
</table>