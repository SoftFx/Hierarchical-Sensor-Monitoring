@model HSMServer.Model.TreeViewModel.SensorNodeViewModel

@{
    var journalParentDivId = $"journal_parent_{@Model.EncodedId}";
}

<div class='tab-pane fade mt-2' name="journal" id=@journalParentDivId>
    <div id="noDataJournal" class="d-flex d-none justify-content-center align-items-center noData-label">
        <label>No data</label>
    </div>
    <table class="display w-100" id="journal_table_@Model.EncodedId">
        <thead>
        <tr>
            <th>Date</th>
            <th>Initiator</th>
            <th>Text</th>
        </tr>
        </thead>
    </table>
</div>