{
  "compilerOptions": {
    "outDir": "./wwwroot/dist",
    "noImplicitAny": true,
    "module": "Node16",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "Node16",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "importHelpers": true
  }
}